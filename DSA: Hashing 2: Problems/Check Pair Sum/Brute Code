class HelloWorld {
    public static boolean solve(int a[], int k) {
        int n = a.length;
        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                if (a[i] + a[j] == k) {
                    return true;
                }
            }
        }
        return false; 
    }
}
