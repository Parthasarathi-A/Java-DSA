class HelloWorld {
    public static int maxk(int[] a, int k) {
        int n = a.length;
        int ans = Integer.MIN_VALUE;
        int i = 0;
        int j = k - 1;
        while (j < n) {
            int sum = 0;
            for (int x = i; x <= j; x++) { // Use a different variable name (x) to avoid conflict
                sum += a[x];
            }
            ans = Math.max(ans, sum);
            i++;
            j++;
        }
        return ans;
    }
