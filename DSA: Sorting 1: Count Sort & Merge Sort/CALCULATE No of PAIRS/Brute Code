public class Main {
    public static int solve(int a[], int b[]) {
        int n = a.length;
        int m = b.length;
        int c = 0;
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++) {
                if (a[i] > b[j]) {
                    c++;
                }
            }
        }
        return c;
    }
}
