import java.util.*;
public class Main{
    public static int solve(int a[], int k){
        int n=a.length;
        for(int i=0;i<n;i++){
            if(a[i]==k){
                return i;
            }
        }
        return -1;
    }
    public static void main(String[] args){
        int a[]={7,8,1,2,3,4,5};
        int k=0;
        System.out.println(solve(a,k));
    }
}
